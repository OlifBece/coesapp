- model_class = Tipoasignatura
.page-header
  %h1=t '.title', default: model_class.model_name.human.titleize
.fieldset
  %dl
    %dt.label.label-default= model_class.human_attribute_name(:descripcion)
    %dd
      %pre.prettyprint= @tipoasignatura.descripcion

.form-group
  = link_to t('.back', default: t("helpers.links.back")), tipoasignaturas_path, class: 'btn btn-default'
  = link_to t('.edit', default: t("helpers.links.edit")), edit_tipoasignatura_path(@tipoasignatura), class: 'btn btn-default'
  = link_to t('.destroy', default: t("helpers.links.destroy")), tipoasignatura_path(@tipoasignatura), method: "delete", data: { confirm: t('.confirm', default: t("helpers.links.confirm", default: 'Are you sure?')) }, class: 'btn btn-danger'