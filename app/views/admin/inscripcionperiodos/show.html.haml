- model_class = Inscripcionperiodo
.page-header
  %h1=t '.title', default: model_class.model_name.human.titleize
.fieldset
  %dl
    %dt.label.label-default= model_class.human_attribute_name(:periodo_id)
    %dd
      %pre.prettyprint= @inscripcionperiodo.periodo_id
    %dt.label.label-default= model_class.human_attribute_name(:estudiante_id)
    %dd
      %pre.prettyprint= @inscripcionperiodo.estudiante_id
    %dt.label.label-default= model_class.human_attribute_name(:tipo_estado_inscripcion_id)
    %dd
      %pre.prettyprint= @inscripcionperiodo.tipo_estado_inscripcion_id

.form-group
  = link_to t('.back', default: t("helpers.links.back")), inscripcionperiodos_path, class: 'btn btn-default'
  = link_to t('.edit', default: t("helpers.links.edit")), edit_inscripcionperiodo_path(@inscripcionperiodo), class: 'btn btn-default'
  = link_to t('.destroy', default: t("helpers.links.destroy")), inscripcionperiodo_path(@inscripcionperiodo), method: "delete", data: { confirm: t('.confirm', default: t("helpers.links.confirm", default: 'Are you sure?')) }, class: 'btn btn-danger'