- model_class = TipoEstadoCalificacion
.page-header
  %h1=t '.title', default: model_class.model_name.human.titleize
.fieldset
  %dl
    %dt.label.label-default= model_class.human_attribute_name(:descripcion)
    %dd
      %pre.prettyprint= @tipo_estado_calificacion.descripcion

.form-group
  = link_to t('.back', default: t("helpers.links.back")), tipo_estado_calificaciones_path, class: 'btn btn-default'
  = link_to t('.edit', default: t("helpers.links.edit")), edit_tipo_estado_calificacion_path(@tipo_estado_calificacion), class: 'btn btn-default'
  = link_to t('.destroy', default: t("helpers.links.destroy")), tipo_estado_calificacion_path(@tipo_estado_calificacion), method: "delete", data: { confirm: t('.confirm', default: t("helpers.links.confirm", default: 'Are you sure?')) }, class: 'btn btn-danger'