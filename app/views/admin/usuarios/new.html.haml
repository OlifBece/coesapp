- if params[:estudiante]
	%h1 Nuevo: Estudiante
- elsif params[:administrador]
	%h1 Nuevo: Administrador
- elsif params[:profesor]
	%h1 Nuevo: Profesor	
- else
	- model_class = Usuario
	.page-header
		%h1=t '.title', default: [:'helpers.titles.new', 'New %{model}'], model: model_class.model_name.human.titleize
= render partial: "form"
